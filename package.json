    "draft-js": "^0.11.7",
    "draftjs-to-markdown": "^0.6.0",
    "formik": "^2.2.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jwt-decode": "^3.1.2",
    "keyboardjs": "^2.6.4",
    "lodash": "^4.17.20",
    "ms": "^2.1.2",
