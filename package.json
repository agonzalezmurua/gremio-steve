    "@types/webpack-dev-server": "^3.11.1",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "autoprefixer": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^2.8.0",
