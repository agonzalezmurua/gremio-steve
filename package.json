    "npm-run-all": "^4.1.5",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "twin.macro": "^2.0.4",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1",
    "webpack": "^5.22.0",
