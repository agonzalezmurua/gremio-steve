    "@formatjs/cli": "^2.13.15",
    "@svgr/webpack": "^5.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/keyboardjs": "^2.5.0",
    "@types/node": "^14.14.8",
    "@types/overlayscrollbars": "^1.12.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
